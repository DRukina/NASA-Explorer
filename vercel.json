{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/$1"
    }
  ],
  "functions": {
    "api/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "outputDirectory": "frontend/dist"
}
